{
	"extends": "airbnb",
	"parser": "babel-eslint",
	"root": true,
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"globals": {
		"describe": false,
		"it": false,
		"require": false
	},
	"rules": {
		"indent": ["error", "tab"],
		"padded-blocks": "off",
		"comma-dangle": "off",
		"arrow-body-style": "off",
		"import/no-unresolved": "off"
	},
	"plugins": [
		"react"
	]
}
